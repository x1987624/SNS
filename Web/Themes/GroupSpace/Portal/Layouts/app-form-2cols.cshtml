<!DOCTYPE html>
<html>
@{ TempData["ShowGroupLogo"] = false;}
@using (Html.BeginHead())
{
    @Styles.Render("~/Bundle/Styles/Site")
    @Html.RenderStyles() 
    @Scripts.Render("~/Bundle/Scripts/jQuery")
}
<body id="@Html.GenerateBodyId()" class="tn-page-bg spb-layout-5-19 spb-group-page">
    <div id="tn-doc" class="tn-doc-bg">
        <div id="tn-header" class="tn-header-bg">
            <div class="tn-wrapper">
                @Html.Action("_Header", "GroupSpaceTheme", new { area = "Group" })
                <div class="spb-po-g-profile">
                    <div class="spb-po-g-profile-wrap">
                        @Html.Action("_GroupHeader", "GroupSpaceTheme", new { spaceKey = Url.SpaceKey(), showManageButton = false, area = "Group" })
                        @Html.Action("_Menu_Main", "GroupSpaceTheme", new { area = "Group" })
                    </div>
                </div>
            </div>
        </div>
        <div id="tn-content" class="tn-content-bg">
            <div class="tn-wrapper">
                <div class="tn-grid-19-5">
                    <div class="tn-unit">
                        <div class="tnc-aside">
                            @RenderSection("Aside", false)
                        </div>
                    </div>
                    <div class="tn-unit tn-first">
                        <div class="tnc-main">
                            @RenderBody()
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div id="tn-footer" class="tn-footer-bg">
            <div class="tn-wrapper">
                @Html.Action("_Footer", "GroupSpaceTheme", new { area = "Group" })
            </div>
        </div>
    </div>
    @Scripts.Render("~/Bundle/Scripts/Site")
    @RenderSection("Scripts", false)
    @Html.RenderScripts()
    @Html.RenderScriptBlocks()
</body>
</html>
