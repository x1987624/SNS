<!DOCTYPE html>
<html>
@using (Html.BeginHead())
{
    @Styles.Render("~/Bundle/Styles/Site")
    @Html.RenderStyles() 
    @Scripts.Render("~/Bundle/Scripts/jQuery")
}
<body id="@Html.GenerateBodyId()" class="tn-page-bg spb-layout-1 spb-group-page">
    <div id="tn-doc" class="tn-doc-bg">
        <div id="tn-header" class="tn-header-bg">
            <div class="tn-wrapper">
                @Html.Action("_Header", "GroupSpaceTheme", new { spaceKey = Url.SpaceKey(), area = "Group" })
            </div>
        </div>
        <div id="tn-content" class="tn-content-bg">
            <div class="tn-wrapper">
                <div class="tn-grid">
                    @RenderBody()
                </div>
            </div>
        </div>
        <div id="tn-footer" class="tn-footer-bg">
            <div class="tn-wrapper">
                @Html.Action("_Footer", "GroupSpaceTheme", new { spaceKey = Url.SpaceKey(), area = "Group" })
            </div>
        </div>
    </div>
    @Scripts.Render("~/Bundle/Scripts/Site")
    @RenderSection("Scripts", false)
    @Html.RenderScripts()
    @Html.RenderScriptBlocks()
</body>
</html>
