@model EmotionCategory
@if (Model != null)
{
    <div class="tnc-dailog-pd">
        <div class="tn-tabs-panel">
            <div class="tn-emotion-list tn-helper-clearfix" style="width: 300px; height: 180px;">
                @foreach (Emotion emotion in Model.Emotions)
                {
                    <span class="tn-border-gray">
                        <img src="@emotion.ImageUrl" alt="@emotion.Description" title="@emotion.Description" value="@emotion.Code" /></span>
                }
            </div>
        </div>
    </div>
    <script type="text/dialog">
      this.title("@(Model.CategoryName)");
    </script>
}
