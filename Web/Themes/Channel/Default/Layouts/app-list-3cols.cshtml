<!DOCTYPE html>
<html>
@using (Html.BeginHead())
{
    @Styles.Render("~/Bundle/Styles/Site")
    @Html.RenderStyles() 
    @Scripts.Render("~/Bundle/Scripts/jQuery")
}
<body id="@Html.GenerateBodyId()" class="tn-page-bg spb-app-page @Html.GenerateBodyAppCssClass()">
    <div id="tn-doc" class="tn-doc-bg">
        <div id="tn-header" class="tn-header-bg">
           <div class="tn-wrapper">
            @Html.Action("_Header", "Channel", new { area = "Common" }, true)
            </div>
        </div>
        <div id="tn-content" class="tn-content-bg">
            <div class="tn-wrapper">
                <div class="tnc-content-header">
                    @if (IsSectionDefined("ContentHeader"))
                    {
                        @RenderSection("ContentHeader", false)
                    }
                    else
                    { 
                        @Html.Action("_ContentHeader", "Channel", new { area = "Common" }, true)
                    }
                </div>
                <div class="tn-grid-5-19">
                    <div class="tn-unit tn-first">
                        <div class="tnc-aside-first">
                            @RenderSection("Aside")
                        </div>
                    </div>
                    <div class="tn-grid-14-5">
                        <div class="tn-unit tn-first">
                            <div class="tnc-main">
                                @RenderBody()
                            </div>
                        </div>
                        <div class="tn-unit">
                            <div class="tnc-aside-second">
                                @RenderSection("AsideSecond")
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div id="tn-footer" class="tn-footer-bg">
            <div class="tn-wrapper">
                @Html.Action("_Footer", "Channel", new { area = "Common" })
            </div>
        </div>
    </div>
    @Scripts.Render("~/Bundle/Scripts/Site")
    @RenderSection("Scripts", false)
    @Html.RenderScripts()
    @Html.RenderScriptBlocks()
</body>
</html>
